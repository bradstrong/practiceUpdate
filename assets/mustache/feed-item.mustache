{{#items}}
<div class="stream-item-container">
	<article class="stream-item in-stream {{#featured}}featured{{/featured}}" data-pu-iid="{{id}}">
		<div class="item-container">
			<div class="item-header-container">
				<header class="item-header">
					{{#flag}}
					<div class="flag-container">
						{{#featured}}<span class="flag featured"><i class="icon-star"></i> featured</span>{{/featured}}
						{{#story-of-the-week}}<span class="flag story-of-the-week"><i class="icon-bookmark"></i> story of the week</span>{{/story-of-the-week}}
					</div>
					{{/flag}}

					<hgroup class="item-title-container">
						<h1 class="item-title"><a href="{{url}}">{{title}}</a></h1>
					</hgroup><!--/.item-title-container-->
					<div class="pub-meta">
						<span class="item-type">{{type}}</span>
						<span class="separator-middot">&middot;</span>
						<span class="item-pub-date">{{#pub-date}}{{date.month}} {{date.day}}, {{date.year}}{{/pub-date}}</span>
						<span class="label label-conference-abbr">ASCO</span>
  					<span class="label label-meta-new">new</span>
  					<span class="label label-meta-free">free</span>
    				<span class="label label-media-video"><i class="icon-youtube-play"></i></span>
					</div><!-- /.pub-meta -->
				</header>
			</div><!--/.item-header-container-->
			{{#item-content}}
			<div class="item-content-container">
				<div class="item-content">
					{{#commentary}}
					<div class="expert-comment-container">
					<div class="comment-meta">
						<h5><i class="icon-comment"></i>&nbsp;Expert Comment</h5>
						{{#source}}
						<dl class="author-list author-list-compact">
    <dt class="list-label">Written by</dt>
    {{#expert}}
    <dd>
      <span class="vcard vcard-inline">
        <img class="avatar a20" src="{{{img.url}}}"/>
        <!-- {{firstName}} {{middleName}} {{lastName}} {{honorific}} -->
        {{#name}}
        <a class="fn n {{source.type}}-name" href="#">
          <!-- <span class="honorific-prefix">Dr.</span> -->
          <span class="given-name">{{firstName}}</span>
          <span class="additional-name">{{middleName}}</span>
          <span class="family-name">{{lastName}}</span>
          <span class="honorific-suffix">{{honorific}}</span>
        </a><!--/.fn-->
        {{/name}}
      </span><!--/.vcard-->
    </dd>
    {{/expert}}
<dl><!-- /.author-list-->
						{{/source}}
						</div><!--/.comment-meta-->
					</div><!--/.expert-comment-container-->
					{{/commentary}}
					<!-- <div class="take-home-message"><p>{{{take-home-message}}}</p></div> --><!--/.take-home-message-->
					<!-- <div class="item-teaser"><p>{{{item-teaser}}}</p></div> --><!--/.item-teaser-->

				</div><!--/.item-content-->
			</div><!--/.item-content-container-->
			{{/item-content}}
			{{#source}}
			<div class="item-source-container">
				<div class="item-source">
					{{#expert}}
					{{! expert opoinion source removed from template. will add once expert opinion has been implemented in juneo }}
					{{/expert}}
					{{#journal}}
					<div class="journal">
						<dl class="source-citation">

							<dt class="journal-title">{{title}}</dt>
							<dt class="item-title"><cite>{{source.title}}</cite></dt>
							<dd class="item-citation"><span class="label label-item-type">ArticleTypeName</span> <span class="separator-middot">&middot;</span> {{source.citation}}</dd>
						</dl>
					</div><!--/.journal-->
					{{/journal}}
					{{#feed}}
					<div class="feed">
						<dl class="source-citation">
							<dt class="feed-title">{{title}}</dt>
							<dd class="item-citation">{{source.citation}}</dd>
						</dl><!--/.source-citation-->
					</div><!--/.feed-->
					{{/feed}}
				</div><!--/.item-source -->
			</div><!--/.item-source-container-->
			{{/source}}

		</div><!--/.item-container-->
		<footer class="item-footer">
			<!-- <div class="portal-meta meta">
			<dl class="rating"><dt class="practice-relevance">Practice Relevance:</dt> <dd class="value">{{practice-relevance}}</dd></dl>
			</div> -->
			<!--
			<div class="item-actions">
			<ul class="link-list">
			<li><a href="#" ><i class="icon-star-empty"></i></a></li>
			<li><a href="#" ><i class="icon-share-alt"></i></a></li>
			</ul>--><!--/.link-list-->
			<!-- </div> --><!--/.item-actions-->
		</footer>
	</article><!--/.stream-item-->
</div><!--/.stream-item-container-->
{{/items}}

